          "regex": "\\+?[\\d\\s\\-\\(\\)]+"
        },
        "email": {
          "selector": ".contact-email, .email-address, [data-email]",
          "attribute": "text"
        },
        "agency": {
          "selector": ".agency-name, .broker-name, .publisher-name",
          "attribute": "text"
        },
        "features": {
          "selector": ".property-features li, .amenities li, .features-list li",
          "attribute": "text"
        }
      },
      "waitTimes": {
        "betweenPages": 2000,
